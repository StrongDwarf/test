        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UseXMLFileDecorateView"></activity>
        <activity android:name=".FragmentShow"></activity>
        <activity android:name=".TabBarActivity"></activity>
        <activity android:name=".ThreadActivity"></activity>
        <activity android:name=".NetworkActivity"></activity>
        <activity android:name=".okHActivity"></activity>
        <activity android:name=".StorageActivity"></activity>
        <activity android:name=".AndroidhighActivity"></activity>
        <activity android:name=".NotificationActivity"></activity>
        <activity android:name=".OftenfuncActivity"></activity>
        <activity android:name=".LauncherActivity"></activity>
        <activity android:name=".FirstLauncherActivity"></activity>
        <activity android:name=".BannerActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

</manifest>